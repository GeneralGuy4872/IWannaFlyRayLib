(defun void lightningbolt {
	(defvar irr::core::vector3df (start . '(0 0 0)))
	(defvar irr::core::vector3df (end))
	(for '((ℓ . 0) (LT ℓ max) (inc ℓ)) {
		(set {end .
			(- (mod (rand) ℓ) (mod (rand) ℓ))
			(- (mod (rand) ℓ) (mod (rand) ℓ))
			ℓ
		)
		(self->draw3DLine start end '(0 255 255 127))
		(set (start . end))
	)
)
	
